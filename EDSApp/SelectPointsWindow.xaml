<Window x:Class="EDSApp.SelectPointsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EDSApp"
        mc:Ignorable="d"
        Title="SelectPointsWindow" Height="300" Width="600">
    <Grid>
        <Grid Name="mainGrid">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="20"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="7*"/>
                <ColumnDefinition Width="3*"/>
            </Grid.ColumnDefinitions>
            <Grid Name="SelectPointGrid" Grid.Row="0" Grid.Column="0">
                <ListBox Name="lbPoints">
                    
                </ListBox>
            </Grid>
            <Grid Name="SelectedPointsGrid" Grid.Row="0" Grid.Column="1">
                
            </Grid>
        </Grid>
    </Grid>
</Window>
